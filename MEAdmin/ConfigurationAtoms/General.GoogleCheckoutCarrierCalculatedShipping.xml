  <Title>Configure Google Checkout Carrier Calculated Shipping</Title>
  <AppConfig Required="false" Advanced="false" FriendlyName="Enable Carrier Calculated Shipping">
    <Name>GoogleCheckout.CarrierCalculatedShippingEnabled</Name>
  </AppConfig>
  <AppConfig Required="false" Advanced="false" FriendlyName="Default Price">
    <Name>GoogleCheckout.CarrierCalculatedDefaultPrice</Name>
  </AppConfig>
  <AppConfig Required="false" Advanced="false" FriendlyName="Free Option">
    <Name>GoogleCheckout.CarrierCalculatedFreeOption</Name>
  </AppConfig>
  <AppConfig Required="false" Advanced="false" FriendlyName="Package">
    <Name>GoogleCheckout.CarrierCalculatedPackage</Name>
  </AppConfig>
  <AppConfig Required="false" Advanced="false" FriendlyName="Option">
    <Name>GoogleCheckout.CarrierCalculatedShippingOptions</Name>
  </AppConfig>
